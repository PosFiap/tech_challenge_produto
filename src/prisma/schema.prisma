// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_HOST")
}

model Produto {
  codigo           Int             @id @default(autoincrement())
  nome             String          
  descricao        String
  valor            Float
  categoria_codigo Int
  
}





